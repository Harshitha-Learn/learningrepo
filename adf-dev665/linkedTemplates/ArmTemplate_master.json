{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-dev665"
		},
		"adls_ls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://stg1devtesti.dfs.core.windows.net/"
		},
		"AzureDataLakeStorage972_properties_privateLinkResourceId": {
			"type": "string",
			"defaultValue": "/subscriptions/97a9a77c-8104-417e-995b-eace2cfd65eb/resourceGroups/rg-new/providers/Microsoft.Storage/storageAccounts/stg1devtesti"
		},
		"AzureDataLakeStorage972_properties_groupId": {
			"type": "string",
			"defaultValue": "dfs"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-dev665_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"adls_ls_properties_typeProperties_url": {
						"value": "[parameters('adls_ls_properties_typeProperties_url')]"
					},
					"AzureDataLakeStorage972_properties_privateLinkResourceId": {
						"value": "[parameters('AzureDataLakeStorage972_properties_privateLinkResourceId')]"
					},
					"AzureDataLakeStorage972_properties_groupId": {
						"value": "[parameters('AzureDataLakeStorage972_properties_groupId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}